font-large()
  font-family YsabeauInfant, monospace
  font-size clamp(1.2rem, calc(1.5vw + 1rem), 1.8rem)
  line-height 1.3
font-medium-large()
  font-family YsabeauInfant, monospace
  font-size clamp(1rem, calc(1.6vw + 0.6rem), 1.5rem)
  line-height 1.3
font-medium()
  font-family YsabeauInfant, monospace
  font-size clamp(0.9rem, calc(1vw + 1rem), 1.3rem)
  line-height 1.3
font-medium-small()
  font-family YsabeauInfant, monospace
  font-size clamp(0.9rem, calc(0.8vw + 0.8rem), 1.2rem)
  font-weight 300
  line-height 1.3
font-small()
  font-family YsabeauInfant, monospace
  font-size clamp(0.8rem, calc(0.5vw + 0.75rem), 1.2rem)
  line-height 1.3
font-small-extra()
  font-family YsabeauInfant, monospace
  font-size clamp(0.8rem, calc(0.2vw + 0.7rem), 1.1rem)
  line-height 1.3


font-special()
  font-family MontserratAlternates, monospace


font-counter()
  font-family GothicE, monospace
  font-size calc(1.9rem)
  line-height 1.3
