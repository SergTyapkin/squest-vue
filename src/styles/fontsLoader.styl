_font_default(fontFamily)
  font-family fontFamily
  font-style normal
  font-stretch normal


@font-face
  _font_default(MontserratAlternates)

  font-weight 300
  src local(MontserratAlternates-Light),
    url("/src/res/fonts/MontserratAlternates/MontserratAlternates-Light.ttf") format("truetype"),
    url("/src/res/fonts/MontserratAlternates/AnyConv.com__MontserratAlternates-Light.woff") format("woff")

